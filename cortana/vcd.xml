<?xml version="1.0" encoding="utf-8"?>
<VoiceCommands xmlns="http://schemas.microsoft.com/voicecommands/1.2">
  <CommandSet xml:lang="en-us" Name="RJS">
    <CommandPrefix>PTO Planner</CommandPrefix>
    <Example>PTO Planner rename to foo</Example>
    <Command Name="Search">
      <Example>rename {message} using PTO Planner</Example>
      <ListenFor RequireAppName="BeforeOrAfterPhrase">Rename to {searchTerm}</ListenFor>
      <Feedback>Rename to {searchTerm} with PTO Planner</Feedback>
      <Navigate/>
    </Command>
    <PhraseTopic Label="searchTerm" Scenario="Dictation"></PhraseTopic>
  </CommandSet>
</VoiceCommands>